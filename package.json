{
  "scripts": {
    "lint": "eslint --no-error-on-unmatched-pattern --config cleaning/.eslintrc.json --ignore-path cleaning/.eslintignore * && prettier --check --config cleaning/.prettierrc.json --ignore-path cleaning/.prettierignore  .",
    "lint:watch": "nodemon -e ts --exec \"clear; yarn lint\"",
    "format": "prettier --config cleaning/.prettierrc.json --ignore-path cleaning/.prettierignore .",
    "dev": "docker compose -f infra/docker/docker-compose.database.yml up",
    "undev": "docker compose -f infra/docker/docker-compose.database.yml down --remove-orphans",
    "start": "docker compose -f infra/docker/docker-compose.database.yml -f infra/docker/docker-compose.yml up -e NODE_ENV=development",
    "stop": "docker compose -f infra/docker/docker-compose.database.yml -f infra/docker/docker-compose.yml down --remove-orphans",
    "build": "sh infra/scripts/build.sh",
    "buildall": "yarn build database & yarn build server & yarn build web & wait; echo \"Lexico built!\"",
    "apply": "sh infra/scripts/apply.sh",
    "upgrade": "sh infra/scripts/upgrade.sh",
    "env": "sh infra/scripts/env.sh"
  },
  "dependencies": {
    "@storybook/addon-essentials": "^6.4.14",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "number-to-words": "^1.2.4",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.31",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@graphql-eslint/eslint-plugin": "^2.0.2",
    "@next/eslint-plugin-next": "^11.1.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "^14.14.11",
    "@types/number-to-words": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "class-validator": "^0.12.2",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.24.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "bugs": {
    "url": "https://github.com/JimmyPaolini/Lexico/issues"
  },
  "homepage": "https://www.lexicolatin.com",
  "name": "lexico",
  "author": "Jimmy Paolini",
  "description": "Latin Language Interface",
  "repository": "https://github.com/JimmyPaolini/Lexico",
  "version": "2.0.0",
  "license": "MIT"
}
